  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@playwright/test": "^1.34.3",
    "@types/dotenv": "^8.2.0",
    "@types/k6": "^0.42.0",
    "@types/webpack": "^5.28.0",
    "allure-playwright": "^2.4.0",
    "babel-loader": "^9.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "k6": "^0.0.0",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-glob-entries": "^1.0.1"
  },
  "scripts": {
    "demo:ui": "ALLURE_RESULTS_DIR=allure-results npx playwright test ./tests/demo.ui.ts -c ./config/ui.playwright.config.ts --reporter=json,list,allure-playwright",
    "demo:api": "npx playwright test ./tests/demo.api.ts -c ./config/api.playwright.config.ts --reporter=json,list,allure-playwright",
    "demo:pt": "npm run build && k6 run --out json=test_results.json dist/demo.pt.js",
    "test:ui": "npx playwright test ./tests/*.ui.ts --reporter=json,list,html",
    "test:api": "npx playwright test ./tests/*.api.ts --reporter=json,list,html",
    "test:pt": "k6 run --out json=test_results.json dist/kds-key.pt.js",
    "build": "npx webpack",
    "report": "allure generate allure-results -o allure-report --clean"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dotenv-cli": "^7.2.1",
    "fs": "^0.0.1-security",
    "pg": "^8.11.0",
    "uuid": "^9.0.0",
    "uuidv4": "^6.2.13"
  }
}
